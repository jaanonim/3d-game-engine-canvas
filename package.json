    "doc": "typedoc --entryPointStrategy expand ./src"
  },
  "devDependencies": {
    "typedoc": "^0.23.15",
    "typedoc-plugin-rename-defaults": "^0.6.4",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}
