{
  "name": "3d-game-engine-canvas",
  "version": "0.1.26",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "npm version --no-git-tag-version patch && npm run doc && npm publish",
